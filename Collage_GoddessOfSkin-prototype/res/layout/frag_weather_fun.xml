<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hanbando"
    android:orientation="vertical" >

    <ImageButton
        android:id="@+id/btn_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="47dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_launcher" />

    <ImageButton
        android:id="@+id/btn_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn_02"
        android:layout_alignParentRight="true"
        android:layout_marginRight="56dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_launcher" />

    <ImageButton
        android:id="@+id/btn_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginRight="16dp"
        android:layout_toLeftOf="@+id/ImageButton01"
        android:scaleType="fitXY"
        android:src="@drawable/ic_launcher" />

    <ImageButton
        android:id="@+id/btn_04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageButton02"
        android:layout_alignLeft="@+id/btn_03"
        android:layout_marginBottom="19dp"
        android:layout_marginLeft="32dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_launcher" />

    <ImageButton
        android:id="@+id/btn_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ImageButton01"
        android:layout_marginTop="22dp"
        android:layout_toRightOf="@+id/btn_02"
        android:scaleType="fitXY"
        android:src="@drawable/ic_launcher" />

</RelativeLayout>