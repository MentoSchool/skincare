<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@android:color/white" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/profile_avatar_padding_outer"
        android:left="@dimen/profile_avatar_padding_outer"
        android:right="@dimen/profile_avatar_padding_outer"
        android:top="@dimen/profile_avatar_padding_outer">
        <shape android:shape="rectangle" >
            <solid android:color="#FF23a1db" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/profile_avatar_padding_inner"
        android:left="@dimen/profile_avatar_padding_inner"
        android:right="@dimen/profile_avatar_padding_inner"
        android:top="@dimen/profile_avatar_padding_inner">
        <shape android:shape="rectangle" >
            <solid android:color="#FFdae7f7" />
        </shape>
    </item>

</layer-list>